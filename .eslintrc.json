{
  "plugins": ["prettier"],
  "globals": {
      "React": "readonly"
  },
  "extends": [
      "next",
      "next/core-web-vitals",
      "eslint:recommended",
      "prettier"
  ],
  "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn",
      "prettier/prettier": "warn",
      "react/no-unescaped-entities": "off",
      "@next/next/no-page-custom-font": "off"
  }
}
